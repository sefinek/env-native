    "dotenv-replacement",
    "util.parseEnv",
    "native",
    "nodejs",
    "zero-dependency",
    "lightweight",
    "minimal",
    "fast",
    "secure",
    "env cli",
    "cli"
  ],
  "homepage": "https://github.com/sefinek/env-native#readme",
  "bugs": {
    "url": "https://github.com/sefinek/env-native/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sefinek/env-native.git"
  },
  "funding": {
    "type": "individual",
    "url": "https://sefinek.net/donate"
  },
  "license": "MIT",
  "author": "Sefinek <contact@sefinek.net> (https://sefinek.net)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js",
      "default": "./index.js"
    }
  },
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "enative": "cli.js",
    "env-native": "cli.js"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
