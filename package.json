  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@jest/globals": "^30.1.2",
    "globals": "^16.3.0",
    "jest": "^30.1.3"
  },
  "engines": {
    "node": ">=20.12.0"
  }
